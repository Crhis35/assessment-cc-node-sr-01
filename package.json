{
  "name": "battle-of-monsters-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ts-node": "cross-env NODE_OPTIONS=\"--loader=ts-node/esm --no-warnings --experimental-specifier-resolution=node\" node",
    "start": "npm run ts-node src/index.ts",
    "predevelop": "npm run db:migrate && npm run db:seed",
    "dev": "nodemon",
    "lint": "eslint src",
    "prettier": "./node_modules/.bin/prettier --write \"{src,knex}/**/*.ts\"",
    "pretest": "cross-env NODE_ENV=test npm run db:rollback:all && cross-env NODE_ENV=test npm run db:migrate",
    "pretest:CI": "cross-env NODE_ENV=test npm run db:migrate",
    "test": "jest --env=node",
    "test:CI": "cross-env NODE_OPTIONS=\"--max-old-space-size=3072\" jest --maxWorkers=2",
    "watch": "jest --watchAll",
    "knex": "npm run ts-node node_modules/knex/bin/cli.js",
    "db:migrate": "npm run knex -- migrate:latest",
    "db:migrate:make": "npm run knex -- migrate:make",
    "db:rollback": "npm run knex -- migrate:rollback",
    "db:rollback:all": "npm run knex -- migrate:rollback --all",
    "db:reset": "npm run db:rollback:all && npm run db:setup",
    "db:seed": "npm run knex -- seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "test:challenge-1": "jest --config=jest.problem-solving.config.js ./problem-solving/challenge-1/index.spec.ts",
    "test:challenge-2": "jest --config=jest.problem-solving.config.js ./problem-solving/challenge-2/index.spec.ts",
    "test:challenge-3": "jest --config=jest.problem-solving.config.js ./problem-solving/challenge-3/index.spec.ts",
    "test:all-challenges": "jest --config=jest.problem-solving.config.js ./problem-solving"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "knex": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "objection": "^3.0.1",
    "sqlite3": "^5.0.11",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/csvtojson": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.0.0",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.7",
    "@types/rosie": "^0.0.40",
    "@types/supertest": "^2.0.12",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^6.6.6",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rosie": "^2.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
